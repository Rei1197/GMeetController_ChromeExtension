{
    "manifest_version": 3,
    "name": "Meet Virtual Background",
    "version": "1.0.1",
    "description": "testing the google meet",
    "icons":{
        "16": "image/eye.png",
        "32":"image/eye-2.png",
        "48":"image/eye-3.png",
        "128":"image/eye-3.png"
    },
    "content_scripts": [
        {
            "js":["scripts/load.js"],
            "matches":[
                "https://meet.google.com/*"
            ]
        }
    ],
"permissions": [
  "activeTab"
],
"web_accessible_resources": [{
  "resources": [ "cs.js" ],
  "matches": ["<all_urls>"]
}
]
}