{
  "manifest_version": 3,
  "name": "ReactJS Extension (Github)",
  "description": "Chrome Extension for Detection (Demo).",
  "version": "1.1.3",
  "icons": {
    "16": "chrome_icon_64.png",
    "32": "chrome_icon_64.png",
    "48": "chrome_icon_64.png",
    "128": "chrome_icon_128.png"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "activeTab",
    "storage",
    "tabCapture"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.google.com/*"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "assests/*",
        "best_web_model.json",
        "group1-shard1of3.bin",
        "group1-shard2of3.bin",
        "group1-shard3of3.bin",
        "https://cdn.jsdelivr.net/npm/@tensorflow/tfjs",
        "https://storage.googleapis.com/tfjs-models/savedmodel/ssdlite_mobilenet_v2/model.json",
        "https://cdn.jsdelivr.net/npm/onnxruntime-web@1.15.0/dist/ort-wasm-simd.wasm"
      ],
      "matches": [
        "https://meet.google.com/*"
      ]
    }
  ]
}