{
    "manifest_version": 3,
    "name": "Meet Extension for Object Detection",
    "description": "Chrome extension to detect objects on Google Meet. (User Media)",
    "version": "1.0.6",
    "icons": {
      "16": "images/video.png",
      "48": "images/video.png",
      "128": "images/video.png"
    },
   "content_scripts":[
    {
      "matches": ["https://meet.google.com/*"],
      "js":["src/script.js"]
    }
   ],
    "permissions": ["activeTab", "storage", "tabs", "notifications"],
    "host_permissions":[
      "https://meet.google.com/*"
    ]
    }
