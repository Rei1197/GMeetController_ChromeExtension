{
    "manifest_version": 3,
    "name": "ReactJS Extension (Development)",
    "description": "The extension for chrome using ReactJS.",
    "version": "1.0.1",
    "icons": {
        "16": "chrome_icon_64.png",
        "32": "chrome_icon_64.png",
        "48": "chrome_icon_64.png",
        "128": "chrome_icon_128.png"
      },
    "host_permissions": ["<all_urls>"],
    "action": {
        "default_title": "Click Me",
        "default_popup": "popup.html"
      },
    "background":{
        "service_worker": "background.js"
      },
    "permissions": ["contextMenus","tabs","activeTab","storage","tabCapture"],
    "content_scripts": [
      {
        "matches": ["https://*.google.com/*"],
        "js": ["contentScript.js"]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": ["https://cdn.jsdelivr.net/npm/@tensorflow/tfjs", "https://storage.googleapis.com/tfjs-models/savedmodel/ssdlite_mobilenet_v2/model.json"],
        "matches": [ "https://meet.google.com/*" ]
      }
    ]
}

